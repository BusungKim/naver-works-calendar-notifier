{
  "manifest_version": 3,
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "js": ["lib/moment-2.9.4.min.js", "scripts/content.js"],
      "matches": [
        "https://calendar.worksmobile.com/main*",
        "https://calendar.navercorp.com/main*"
      ],
      "runAt": "document_idle"
    }
  ]
}


