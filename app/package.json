{
  "name": "my-app",
  "version": "1.0.0",
  "targets": {
    "index": {
      "source": "src/index.html",
      "publicUrl": "./",
      "distDir": "../dist"
    },
    "background": {
      "source": "src/background.js",
      "distDir": "../dist"
    },
    "content": {
      "source": "src/content.js",
      "distDir": "../dist"
    }
  },
  "scripts": {
    "clean": "rm -rf .parcel-cache ../dist",
    "eslint": "eslint --fix --ext .jsx,.js,.tsx,.ts src",
    "dev": "parcel",
    "build-local": "npm run clean && cross-env LOCAL_BUILD=true parcel build --no-content-hash --no-optimize",
    "build": "npm run clean && npm run eslint && parcel build --detailed-report --no-source-maps"
  },
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.4",
    "@mui/material": "^5.14.18",
    "moment": "^2.29.4",
    "parcel": "^2.10.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.54.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "process": "^0.11.10"
  }
}
